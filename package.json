{
  "name": "b-o-a",
  "description": "b-o-a",
  "version": "0.0.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/b-o-a/issues"
  },
  "dependencies": {
    "rxjs": "^5.0.0-beta.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "power-assert": "^1.2.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.3",
    "watch": "^0.17.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/bouzuya/b-o-a#readme",
  "keywords": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/b-o-a"
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-es2015 && npm run cp",
    "build-es2015": "babel --presets es2015 --out-dir .tmp/es5/ .tmp/es2015/ >/dev/null",
    "build-ts": "tsc",
    "clean": "rm -rf lib/ .tmp/",
    "cp": "cp -r .tmp/es5/src/ lib/ && cp -r .tmp/es2015/src/*.d.ts lib/",
    "prepublish": "typings install && npm run clean && npm run build",
    "test": "npm run build && mocha --recursive --require intelli-espower-loader .tmp/es5/test/",
    "watch": "npm run build && parallelshell 'npm run watch-ts' 'npm run watch-es2015' 'npm run watch-cp'",
    "watch-cp": "watch --ignoreUnreadable --wait 2 'npm run cp' .tmp/es5/src/",
    "watch-es2015": "watch --ignoreUnreadable --wait 2 'npm run build-es2015' .tmp/es2015/src/",
    "watch-test": "mocha --watch --recursive --require intelli-espower-loader .tmp/es5/test/",
    "watch-ts": "tsc --watch"
  }
}
